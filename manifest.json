{
    "manifest_version": 3,
    "name": "Keybind Extension",
    "version": "1.3",
    "description": "Navigate pages using custom keybinds.",
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y"
            },
            "description": "Execute custom action"
        }
    }
}
